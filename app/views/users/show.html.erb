<h1>User info</h1>
<%= image_tag @user.get_profile_image(100,100)%>
<%= @user.name%>
<%= @user.introduction %>
<%= link_to '設定',edit_user_path %>

<%= render 'books/new',book:@book %>

<h1>Books</h1>
<%= @books.each do |book| %>
<%= image_tag book.user.get_profile_image(100,100) %>
<%= book.user.name%>
<%= link_to'f.text_field:title',book_path %>
<%= f.text_area:body %>

<% end %>